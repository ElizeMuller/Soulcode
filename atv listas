List<String> bandas = new List<String>{
   'O Teatro Mágico' , 'Coldplay' , 'Imagine Dragons' , 'Mr.Big' , 'Twenty One Pilots'  
};
    
System.debug(bandas);

bandas.remove(4);
bandas.remove(3);

System.debug(bandas);
bandas.add('The Head and The Heart');
bandas.add('Radiohead');
bandas.add('Queen');

System.debug(bandas);
